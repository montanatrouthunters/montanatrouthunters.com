    "@astrolib/seo": "1.0.0-beta.8",
    "@fontsource-variable/inter": "5.1.0",
    "@iconify-json/flat-color-icons": "1.2.0",
    "@iconify-json/mdi": "1.2.0",
    "@iconify-json/tabler": "1.2.4",
    "astro-compress": "2.3.1",
    "@tailwindcss/typography": "0.5.15",
    "astro": "4.15.11",
    "astro-embed": "0.7.2",
    "astro-icon": "1.1.1",
    "limax": "4.1.0",
    "lodash.merge": "4.6.2",
